.pos 0
	irmovl stack, %esp
	irmovl stack, %ebp
	call main
	halt
this_func:

	rrmovl %esp, %ebp
	irmovl 0, %ecx
	subl %ecx, %esp
main:

	rrmovl %esp, %ebp
	irmovl 0, %ecx
	subl %ecx, %esp
	irmovl 0, %edi
	rrmovl %ebp, %esp
	ret
.pos 0x74    #Start of string space

.pos 0x78   # Start of global var space 
.pos 0x0000FFFF
stack:
